{
  "extends": [
    "next/core-web-vitals"
  ],
  "plugins": [
    "eslint-plugin-react"
  ],
  "rules": {
      "consistent-return": 2,
      "indent"           : [1, 2],
      "no-else-return"   : 1,
      "semi"             : [1, "never"],
      "space-unary-ops"  : 2,
      "quotes"           : ["error", "single"],
      "indent-legacy"     :"off"
  }
}
